{
  "name": "@hariom-jha/rate-limiter",
  "version": "1.0.0",
  "description": "rate limiter with Express/Fastify middleware",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand",
    "lint": "eslint . --ext .js",
    "pretest": "npm run lint",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --testPathPatterns=redis",
    "test:all": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --testPathIgnorePatterns=[]",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "release": "semantic-release"
  },
  "keywords": [
    "rate-limit",
    "token-bucket",
    "redis",
    "express",
    "fastify"
  ],
  "author": "HARIOM-JHA01",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HARIOM-JHA01/rate-limiter.git"
  },
  "bugs": {
    "url": "https://github.com/HARIOM-JHA01/rate-limiter/issues"
  },
  "homepage": "https://github.com/HARIOM-JHA01/rate-limiter#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "express": ">=4.17.0",
    "fastify": ">=4.0.0"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "dependencies": {
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.6",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "semantic-release": "^21.1.2"
  }
}